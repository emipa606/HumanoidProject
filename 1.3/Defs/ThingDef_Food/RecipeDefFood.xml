<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <RecipeDef Name="SpecialRaceMeal" Abstract="True">
    <fixedIngredientFilter>
      <categories>
        <li>FoodRaw</li>
      </categories>
      <specialFiltersToDisallow>
        <li>AllowPlantFood</li>
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <defaultIngredientFilter>
      <categories>
        <li>FoodRaw</li>
      </categories>
      <disallowedCategories>
        <li>EggsFertilized</li>
      </disallowedCategories>
      <disallowedThingDefs>
        <li>Meat_Human</li>
        <li>Meat_Megaspider</li>
        <li>InsectJelly</li>
      </disallowedThingDefs>
    </defaultIngredientFilter>
    <workSkill>Cooking</workSkill>
  </RecipeDef>
  <!--====== X FoodProduction ======-->
  <RecipeDef ParentName="SpecialRaceMeal">
    <defName>CookAlienMeatRed</defName>
    <label>Fabricate Volcanic Fawn meat</label>
    <description>Fabricate a Strange meat with a unique black and red Color that has normal properties similar to regular common meat. (Can be Produced in Early stages of the game, Unlocked from Tier 1 of Alien Plants)</description>
    <jobString>Cooking Volcanic Fawn meat.</jobString>
    <workSpeedStat>CookSpeed</workSpeedStat>
    <effectWorking>AlienRecipeNormal</effectWorking>
    <soundWorking>Recipe_CookMeal</soundWorking>
    <allowMixingIngredients>True</allowMixingIngredients>
    <workAmount>500</workAmount>
    <ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
    <recipeUsers>
      <li>BioLab</li>
    </recipeUsers>
    <fixedIngredientFilter>
    </fixedIngredientFilter>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>AlienPlantRaw</li>
          </categories>
        </filter>
        <count>5.0</count>
      </li>
    </ingredients>
    <products>
      <AlienMeatRed>5</AlienMeatRed>
    </products>
    <workSkill>Cooking</workSkill>
    <researchPrerequisite>FoodProduction</researchPrerequisite>
  </RecipeDef>
