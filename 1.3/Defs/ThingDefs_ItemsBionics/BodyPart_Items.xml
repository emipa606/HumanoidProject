      <RecipeDef>InstallAlienJaw</RecipeDef>
    </descriptionHyperlinks>
    <graphicData>
      <texPath>Things/Prosthetics/AlienJaw</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <costList>
      <Steel>70</Steel>
      <ComponentSpacer>5</ComponentSpacer>
