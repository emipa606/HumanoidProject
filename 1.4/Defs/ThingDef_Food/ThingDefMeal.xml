  <!--====== X FoodProduction ======-->
  <ThingDef ParentName="SpecialRottable">
    <defName>AlienMeatRed</defName>
    <label>Volcanic Fawn</label>
    <description>A simple, and Strange Meat with a Red color that has normal properties similar to the regular common meat. (Can be Produced in Early stages of the game, Unlocked from Tier 1 of Alien Plants)</description>
    <graphicData>
      <texPath>Things/Food/AlienMeat_0/AlienMeatRed</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <MarketValue>12</MarketValue>
      <Nutrition>0.6</Nutrition>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.5</Flammability>
      <Beauty>5</Beauty>
      <Mass>0.25</Mass>
      <DeteriorationRate>1</DeteriorationRate>
    </statBases>
    <stackLimit>50</stackLimit>
    <ingestible>
      <tasteThought>LabTaste</tasteThought>
      <preferability>MealSimple</preferability>
      <optimalityOffsetHumanlikes>-1</optimalityOffsetHumanlikes>
      <ingestEffect>EatMeat</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <tradeability>Buyable</tradeability>
  </ThingDef>
