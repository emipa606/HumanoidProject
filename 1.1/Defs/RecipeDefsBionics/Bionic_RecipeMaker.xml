<Defs>

	<!-- Not required anymore, the item def generates it automatically.
		 These are debug recipes to prevent sudden disappearances of existing bills 
		 No new recipe defs of these exact defnames will appear for the player,
		 and they will be using the correct item def versions of it. 
		 
		 Recommend to remove in next build for reduction of unneccessary stuff -->

	<RecipeDef Name="HumanoidProject_Debug_BionicRecipe" Abstract="True">
		<workSkill>Crafting</workSkill>
		<workSpeedStat>GeneralLaborSpeed</workSpeedStat>
		<effectWorking>AlienRecipeNormal</effectWorking>
		<soundWorking>Recipe_Machining</soundWorking>
		<unfinishedThingDef>UnfinishedPart</unfinishedThingDef>
		<factionPrerequisiteTags>
			<li>HumanoidProject_RecipeRedundancy_Debug</li>
		</factionPrerequisiteTags>
	</RecipeDef>

	<RecipeDef ParentName="HumanoidProject_Debug_BionicRecipe">
		<defName>MakeAlienJaw</defName>
		<label>Make Cosmos-Tech jaw</label>
		<description>Upgrades to a Cosmos-Tech jaw.</description>
		<descriptionHyperlinks><ThingDef>AlienJaw</ThingDef></descriptionHyperlinks>
		<jobString>Upgrading Body Part</jobString>
		<workAmount>40000</workAmount>
		<ingredients>
		  <li>
			<filter>
			  <thingDefs>
				<li>Steel</li>
			  </thingDefs>
			</filter>
			<count>70</count>
		  </li>
		  <li>
			<filter>
			  <thingDefs>
				<li>ComponentSpacer</li>
			  </thingDefs>
			</filter>
			<count>5</count>
		  </li>
		</ingredients>
		<fixedIngredientFilter>
		  <thingDefs>
			<li>Steel</li>
			<li>ComponentSpacer</li>
		  </thingDefs>
		</fixedIngredientFilter>
		<products>
		  <AlienJaw>1</AlienJaw>
		</products>
		<skillRequirements>
		  <Crafting>10</Crafting>
		</skillRequirements>
	</RecipeDef>

