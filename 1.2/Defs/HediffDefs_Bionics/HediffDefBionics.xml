	<!-- Tier 1 Cosmos-Tech -->
	
	<HediffDef ParentName="CreativePartsBody">
		<defName>AddAlienJaw</defName>
		<label>Cosmos-Tech jaw</label>
		<labelNoun>a Cosmos-Tech jaw</labelNoun>
		<description>An installed Cosmos-Tech jaw.</description>
		<descriptionHyperlinks><ThingDef>AlienJaw</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>AlienJaw</spawnThingOnRemoved>
		<initialSeverity>0.0</initialSeverity>
		<maxSeverity>1.5</maxSeverity>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.0</partEfficiency>
		</addedPartProps>
		<comps>
			<li Class="HediffCompProperties_SeverityPerDay">
				<severityPerDay>0.5</severityPerDay>
			</li>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>Bionic Teeth</label>
						<capacities>
							<li>ToxicBite</li>
						</capacities>
						<power>10</power>
						<cooldownTime>1.5</cooldownTime>
						<linkedBodyPartsGroup>Teeth</linkedBodyPartsGroup>
						<chanceFactor>0.25</chanceFactor>
					</li>
				</tools>
			</li>
		</comps>
		<stages>
			<li>
				<minSeverity>1.0</minSeverity>
				<label>Installed</label>
				<painFactor>0.1</painFactor>
				<statOffsets>
					<PawnBeauty>1</PawnBeauty>
				</statOffsets>
				<capMods>
					<li>
						<capacity>Talking</capacity>
						<offset>0.15</offset>
					</li>
				</capMods>
			</li>
			<li>
				<minSeverity>1.5</minSeverity>
				<label>Fixed</label>
				<statOffsets>
					<PawnBeauty>1</PawnBeauty>
				</statOffsets>
